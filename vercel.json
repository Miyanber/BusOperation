{
	"version": 2,
	"regions": [
        "hnd1"
    ],
	"builds": [
		{
			"src": "busOperation/wsgi.py",
			"use": "@vercel/python",
			"config": {
				"maxLambdaSize": "15mb",
				"runtime": "python3.10"
			}
		},
		{
			"src": "build_files.sh",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "staticfiles"
			}
		}
	],
	"routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "busOperation/wsgi.py"
        }
    ],
	"headers": [
		{
			"source": "/static/(.*)",
			"headers": [
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=0, s-maxage=86400"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=0, s-maxage=86400"
				},
				{
					"key": "Cache-Control",
					"value": "max-age=0, s-maxage=86400"
				}
			]
		}
	]
}